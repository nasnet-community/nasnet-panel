{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist",
    "tsBuildInfoFile": "dist/tsconfig.app.tsbuildinfo",
    "jsx": "react-jsx",
    "lib": ["dom"],
    "types": [
      "node",
      "@nx/react/typings/cssmodule.d.ts",
      "@nx/react/typings/image.d.ts",
      "vite/client"
    ],
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "emitDeclarationOnly": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@nasnet/core/*": ["../../libs/core/*/src"],
      "@nasnet/core/types/*": ["../../libs/core/types/src/*"],
      "@nasnet/ui/*": ["../../libs/ui/*/src"],
      "@nasnet/ui/primitives/*": ["../../libs/ui/primitives/src/*"],
      "@nasnet/ui/patterns/*": ["../../libs/ui/patterns/src/*"],
      "@nasnet/ui/patterns/motion": ["../../libs/ui/patterns/src/motion/index.ts"],
      "@nasnet/ui/layouts/*": ["../../libs/ui/layouts/src/*"],
      "@nasnet/ui/tokens": ["../../libs/ui/tokens/src/index.ts"],
      "@nasnet/features/*": ["../../libs/features/*/src"],
      "@nasnet/features/*/schemas": ["../../libs/features/*/src/schemas"],
      "@nasnet/api-client/*": ["../../libs/api-client/*/src"],
      "@nasnet/api-client/generated": ["../../libs/api-client/generated/index.ts"],
      "@nasnet/api-client/generated/*": ["../../libs/api-client/generated/*"],
      "@nasnet/api-client/queries/*": ["../../libs/api-client/queries/src/*"],
      "@nasnet/state/*": ["../../libs/state/*/src"]
    }
  },
  "exclude": [
    "out-tsc",
    "dist",
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/*.spec.tsx",
    "**/*.test.tsx",
    "**/*.spec.js",
    "**/*.test.js",
    "**/*.spec.jsx",
    "**/*.test.jsx",
    "**/__tests__/**",
    "../../libs/**/*.test.ts",
    "../../libs/**/*.test.tsx",
    "../../libs/**/*.spec.ts",
    "../../libs/**/*.spec.tsx",
    "../../libs/**/__tests__/**"
  ],
  "include": ["src/**/*.js", "src/**/*.jsx", "src/**/*.ts", "src/**/*.tsx"],
  "references": []
}
