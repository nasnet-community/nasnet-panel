{
  "id": "mtproxy",
  "name": "MTProxy",
  "description": "Official Telegram MTProto proxy server. Helps users access Telegram in regions where it's blocked, with support for promoted channels and fake TLS.",
  "version": "1.0.0",
  "category": "Messaging",
  "author": "NasNetConnect",
  "license": "MIT",
  "homepage": "https://github.com/TelegramMessenger/MTProxy",
  "icon": "mtproxy-icon.svg",
  "tags": ["telegram", "proxy", "mtproto", "messaging", "censorship-resistance"],
  "architectures": ["amd64", "arm64"],
  "min_routeros_version": "7.0",
  "required_packages": [],
  "required_ports": [443, 8888],
  "required_memory_mb": 32,
  "required_disk_mb": 64,
  "min_ram": 10485760,
  "recommended_ram": 20971520,
  "cpu_weight": 10,
  "docker_image": "nasnetconnect/mtproxy",
  "docker_tag": "latest",
  "docker_pull_policy": "if-not-present",
  "environment_vars": {
    "SECRET": "",
    "TAG": "",
    "PORT": "443"
  },
  "volumes": [
    {
      "source": "/data/mtproxy",
      "target": "/data",
      "read_only": false
    }
  ],
  "ports": [
    {
      "container_port": 443,
      "host_port": 443,
      "protocol": "tcp"
    },
    {
      "container_port": 8888,
      "host_port": 8888,
      "protocol": "tcp"
    }
  ],
  "network_mode": "bridge",
  "config_schema": {
    "type": "object",
    "properties": {
      "secret": {
        "type": "string",
        "description": "Proxy secret (32 hex chars, auto-generated if empty)"
      },
      "tag": {
        "type": "string",
        "description": "Optional promoted channel tag"
      },
      "port": {
        "type": "integer",
        "minimum": 1,
        "maximum": 65535,
        "description": "Listening port"
      },
      "fake_tls": {
        "type": "boolean",
        "description": "Enable fake TLS mode"
      }
    }
  },
  "default_config": {
    "secret": "",
    "tag": "",
    "port": 443,
    "fake_tls": true
  },
  "pre_install_script": "",
  "post_install_script": "",
  "pre_uninstall_script": "",
  "health_check_script": "nc -z localhost 443 || exit 1",
  "readme_url": "https://github.com/nasnetconnect/mtproxy/blob/main/README.md",
  "changelog_url": "https://github.com/nasnetconnect/mtproxy/blob/main/CHANGELOG.md",
  "support_url": "https://github.com/nasnetconnect/mtproxy/issues"
}
