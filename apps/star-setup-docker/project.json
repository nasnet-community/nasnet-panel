{
  "name": "star-setup-docker",
  "sourceRoot": "apps/star-setup-docker/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/star-setup-docker",
        "configFile": "apps/star-setup-docker/vite.config.ts"
      },
      "configurations": {
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "star-setup-docker:build",
        "port": 5174
      },
      "configurations": {
        "development": {
          "buildTarget": "star-setup-docker:build",
          "hmr": true
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "options": {
        "buildTarget": "star-setup-docker:build",
        "port": 4301
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/star-setup-docker/src/**/*.{ts,tsx}"]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --project apps/star-setup-docker/tsconfig.app.json"
      }
    },
    "check": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx run star-setup-docker:typecheck",
          "nx run star-setup-docker:lint"
        ],
        "parallel": false
      }
    }
  },
  "tags": ["type:app", "framework:qwik", "scope:star-setup"]
}

