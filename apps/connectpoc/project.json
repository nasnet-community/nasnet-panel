{
  "$schema": "https://json.schemastore.org/nx-project.json",
  "name": "connectpoc",
  "projectType": "application",
  "root": "apps/connectpoc",
  "sourceRoot": "apps/connectpoc/src",
  "implicitDependencies": ["shared"],
  "targets": {
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "connectpoc:build",
        "cwd": "apps/connectpoc"
      },
      "configurations": {
        "development": {
          "buildTarget": "connectpoc:build"
        }
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{workspaceRoot}/dist/apps/connectpoc"],
      "options": {
        "outputPath": "dist/apps/connectpoc",
        "cwd": "apps/connectpoc"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "cwd": "apps/connectpoc"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run connectpoc:serve"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/connectpoc/**/*.ts", "apps/connectpoc/**/*.tsx"]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --project apps/connectpoc/tsconfig.app.json"
      }
    },
    "check": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx run connectpoc:typecheck",
          "nx run connectpoc:lint"
        ],
        "parallel": false
      }
    }
  },
  "tags": ["type:app", "scope:frontend"]
}


