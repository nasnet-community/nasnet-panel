{
  "name": "@nas-net/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ci": "nx run-many -t lint test build typecheck",
    "dev:frontend": "nx serve connect",
    "dev:backend": "nx serve backend",
    "dev:verbose": "nx serve connect --verbose",
    "dev:all": "nx run-many --target=serve --projects=connect,backend --parallel --verbose",
    "build:frontend": "nx build connect",
    "build:check": "nx build connect && node scripts/check-bundle-size.mjs",
    "build:analyze": "nx build connect --mode analyze",
    "build:all": "nx run-many -t build",
    "lint": "nx run-many -t lint && npm run lint:css",
    "lint:all": "nx run-many -t lint --all && npm run lint:css",
    "lint:fix": "nx run-many -t lint --fix && npm run lint:css:fix",
    "lint:css": "stylelint \"**/*.{css,scss}\" --ignore-path .stylelintignore",
    "lint:css:fix": "stylelint \"**/*.{css,scss}\" --fix --ignore-path .stylelintignore",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,scss,html,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css,scss,html,yaml,yml}\"",
    "typecheck": "nx run-many -t typecheck",
    "typecheck:all": "nx run-many -t typecheck --all",
    "check": "nx run-many -t typecheck lint --parallel=false",
    "check:all": "nx run-many -t typecheck lint --all --parallel=false --verbose",
    "docker:local": "nx run backend:docker:build-local",
    "docker:export": "nx run backend:docker:export-tar",
    "docker:build-export": "nx run backend:docker:build-export",
    "docker:check-size": "node scripts/check-docker-size.mjs",
    "prepare": "husky",
    "test": "nx run-many -t test --parallel=3",
    "test:watch": "nx run connect:test --watch",
    "test:coverage": "nx run-many -t test --coverage",
    "test:ui": "nx run connect:test --ui",
    "test:affected": "nx affected -t test",
    "test:go": "cd apps/backend && go test -v -tags=dev ./...",
    "test:go:coverage": "cd apps/backend && go test -v -tags=dev -coverprofile=coverage.out ./... && go tool cover -html=coverage.out -o coverage.html",
    "e2e": "nx run connect-e2e:e2e",
    "e2e:ui": "nx run connect-e2e:e2e --ui",
    "generate": "nx generate @nasnet/tools",
    "g:component": "nx generate @nasnet/tools:component",
    "g:library": "nx generate @nasnet/tools:library",
    "g:resolver": "nx generate @nasnet/tools:resolver",
    "tokens:build": "node libs/ui/tokens/build.js",
    "tokens:watch": "node libs/ui/tokens/build.js --watch",
    "codegen": "npm run codegen:ts && npm run codegen:go",
    "codegen:ts": "graphql-codegen --config codegen.ts",
    "codegen:go": "cd apps/backend && go generate ./...",
    "codegen:check": "npm run codegen && git diff --exit-code libs/api-client/generated/ apps/backend/graph/"
  },
  "private": true,
