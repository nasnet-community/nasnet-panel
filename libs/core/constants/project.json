{
  "name": "@nasnet/core-constants",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/core/constants/src",
  "projectType": "library",
  "tags": ["type:lib", "scope:core"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/core/constants/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "tsc --noEmit --project libs/core/constants/tsconfig.lib.json",
          "tsc --noEmit --project libs/core/constants/tsconfig.spec.json",
          "tsc --noEmit --project libs/core/constants/tsconfig.storybook.json"
        ],
        "parallel": true
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/core/constants"
      }
    }
  }
}
