{
  "name": "@nasnet/core-i18n",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/core/i18n/src",
  "projectType": "library",
  "tags": ["type:lib", "scope:core"],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/core/i18n"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "coverage/libs/core/i18n"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/core/i18n/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "tsc --noEmit --project libs/core/i18n/tsconfig.lib.json",
          "tsc --noEmit --project libs/core/i18n/tsconfig.spec.json",
          "tsc --noEmit --project libs/core/i18n/tsconfig.storybook.json"
        ],
        "parallel": true
      }
    }
  }
}
