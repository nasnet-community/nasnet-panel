{
  "name": "@nasnet/ui-tokens",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ui/tokens/src",
  "projectType": "library",
  "tags": ["type:lib", "scope:ui", "scope:shared"],
  "targets": {
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --project libs/ui/tokens/tsconfig.lib.json"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node libs/ui/tokens/build.js",
        "cwd": "{workspaceRoot}"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node libs/ui/tokens/build.js --watch",
        "cwd": "{workspaceRoot}"
      }
    }
  }
}
