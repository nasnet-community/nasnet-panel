{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../../dist/out-tsc",
    "declaration": true,
    "declarationMap": true,
    "types": ["vite/client"],
    "composite": false,
    "tsBuildInfoFile": "../../../dist/out-tsc/ui-layouts.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@nasnet/ui/*": ["../*/src"],
      "@nasnet/core/*": ["../../core/*/src"],
      "@nasnet/state/*": ["../../state/*/src"],
      "@nasnet/api-client/*": ["../../api-client/*/src"],
      "@nasnet/features/*": ["../../features/*/src"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": [
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "src/**/*.spec.tsx",
    "src/**/*.test.tsx"
  ],
  "references": []
}
