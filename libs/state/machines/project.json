{
  "name": "@nasnet/state/machines",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/state/machines/src",
  "projectType": "library",
  "tags": ["type:lib", "scope:state"],
  "targets": {
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/libs/state/machines"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/libs/state/machines"
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/state/machines"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["libs/state/machines/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --project libs/state/machines/tsconfig.lib.json"
      }
    },
    "check": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx run @nasnet/state/machines:typecheck",
          "nx run @nasnet/state/machines:lint"
        ],
        "parallel": false
      }
    }
  }
}
