{
  "name": "@nasnet/features/dashboard",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/features/dashboard/src",
  "projectType": "library",
  "tags": ["type:lib", "scope:features"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["libs/features/dashboard/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "tsc --noEmit --project libs/features/dashboard/tsconfig.lib.json",
          "tsc --noEmit --project libs/features/dashboard/tsconfig.spec.json",
          "tsc --noEmit --project libs/features/dashboard/tsconfig.storybook.json"
        ],
        "parallel": true
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/libs/features/dashboard"],
      "options": {
        "config": "libs/features/dashboard/vitest.config.ts",
        "passWithNoTests": false,
        "reportsDirectory": "coverage/libs/features/dashboard"
      }
    }
  }
}
