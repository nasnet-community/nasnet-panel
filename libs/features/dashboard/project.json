{
  "name": "@nasnet/features/dashboard",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/features/dashboard/src",
  "projectType": "library",
  "tags": ["type:lib", "scope:features"],
  "targets": {
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --project libs/features/dashboard/tsconfig.lib.json"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/libs/features/dashboard"],
      "options": {
        "config": "libs/features/dashboard/vitest.config.ts",
        "passWithNoTests": false,
        "reportsDirectory": "coverage/libs/features/dashboard"
      }
    }
  }
}
